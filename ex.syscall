.data
frase: .asciiz "Entre com sua idade: " #sempre que usar o syscall tem que usar o 'asciiz'
frase_maior: .asciiz "Você é maior de idade"
frase_menor: .asciiz "Você é menor de idade"
.text
li $v0, 4 #entrar com a idade
la $a0, frase
syscall
li $v0, 5
syscall
add $t1, $v0, $0
bge $t1, 18, maior #vejo de $t1 é maior que 18
li $v0, 4
la $a0, frase_menor
syscall
j fim
maior:
li $v0, 4
la $a0, frase_maior
syscall
fim: li $v0,10
     syscall